<script src="../public/js/docs.js"></script>

<script language="JavaScript">
    menuActive("<%= dirname %>");
    function getSearch() {
        if($("input[name=q]").val().trim()!='') {
            location.href = "./search?q=" + $("input[name=q]").val();
        }
    }
</script>
